{
  "info": {
    "version": "1.0.0",
    "title": "SleekFlow Todo API"
  },
  "host": "localhost:4000",
  "definitions": {
    "Todo": {
      "description": "A todo item",
      "example": {
        "added_at": "2025-04-09T11:25:30.867826Z",
        "description": "description new",
        "due_date": "2025-04-14T15:28:42.596658Z",
        "id": "02ef07e0-eb4f-4fca-b6aa-c7993427cc10",
        "name": "test new",
        "priority": "high",
        "status": "in_progress",
        "tags": [
          "test1",
          "test3"
        ],
        "updated_at": "2025-04-13T07:05:25.345643Z"
      },
      "properties": {
        "added_at": {
          "description": "todo item added at",
          "type": "string"
        },
        "description": {
          "description": "todo item description",
          "type": "string"
        },
        "due_date": {
          "description": "todo item due date",
          "type": "string"
        },
        "id": {
          "description": "Unique identifier",
          "type": "string"
        },
        "name": {
          "description": "todo item name",
          "type": "string"
        },
        "priority": {
          "description": "todo item priority",
          "type": "string"
        },
        "status": {
          "description": "todo item status",
          "type": "string"
        },
        "tags": {
          "description": "todo item tags",
          "type": "array"
        },
        "updated_at": {
          "description": "todo item updated at",
          "type": "string"
        }
      },
      "required": [
        "status",
        "name",
        "id"
      ],
      "title": "Todo",
      "type": "object"
    },
    "Todos": {
      "description": "A collection of Todos",
      "items": {
        "$ref": "#/definitions/Todo"
      },
      "title": "Todos",
      "type": "array"
    }
  },
  "paths": {
    "/todos": {
      "get": {
        "description": "Retrieve a list of all todos",
        "operationId": "SleekFlowTodoWeb.TodoController.index",
        "parameters": [
          {
            "description": "Status filter",
            "in": "query",
            "items": {
              "enum": [
                "not_started",
                "in_progress",
                "completed"
              ],
              "type": "string"
            },
            "name": "status",
            "required": false,
            "type": "array"
          },
          {
            "description": "DateTime in ISO8601",
            "in": "query",
            "name": "due_date",
            "required": false,
            "type": "string",
            "x-example": "2025-04-15T15:25:11.550132Z"
          },
          {
            "description": "Sort by",
            "in": "query",
            "name": "sort_by",
            "required": false,
            "type": "string",
            "x-example": "priority"
          },
          {
            "description": "Sort order",
            "in": "query",
            "name": "sort_order",
            "required": false,
            "type": "string",
            "x-example": "asc"
          }
        ],
        "produces": [
          [
            "application/json"
          ]
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        },
        "summary": "List all todos",
        "tags": [
          "Todos"
        ]
      },
      "post": {
        "consumes": [
          [
            "application/json"
          ]
        ],
        "description": "Add a new todo item to the list",
        "operationId": "SleekFlowTodoWeb.TodoController.create",
        "parameters": [
          {
            "description": "Todo object to create",
            "in": "body",
            "name": "todo",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Todo"
            }
          }
        ],
        "produces": [
          [
            "application/json"
          ]
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": {
              "$ref": "#/definitions/Todo"
            }
          },
          "422": {
            "description": "Unprocessable Entity"
          }
        },
        "summary": "Create a new todo",
        "tags": [
          "Todos"
        ]
      }
    },
    "/todos/:id": {
      "get": {
        "description": "Retrieve a specific todo item by its ID",
        "operationId": "SleekFlowTodoWeb.TodoController.show",
        "parameters": [
          {
            "description": "Todo ID",
            "in": "path",
            "name": "id",
            "required": true,
            "type": "string",
            "x-example": "02ef07e0-eb4f-4fca-b6aa-c7993427cc10"
          }
        ],
        "produces": [
          [
            "application/json"
          ]
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/Todo"
            }
          },
          "404": {
            "description": "Not Found"
          }
        },
        "summary": "Show a specific todo",
        "tags": [
          "Todos"
        ]
      }
    }
  },
  "swagger": "2.0",
  "basePath": "/api",
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "tags": [
    {
      "name": "Todos",
      "description": "Operations related to Todos"
    }
  ]
}